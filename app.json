{
  "expo": {
    "name": "PooTime Adventure",
    "slug": "pootime-adventure-mobile-app",
    "version": "5.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "scheme": "fr.bigstones.pootime-adventure",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/icon.png",
      "package": "fr.pierrot.pootimeadventure",
      "versionCode": 5
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-system-ui",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3020955535400199~5556255643",
          "iosAppId": "ca-app-pub-3020955535400199~5556255643"
        }
      ],
      [
        "expo-navigation-bar",
        {
          "visibility": "hidden",
          "behavior": "inset-swipe",
          "position": "relative"
        }
      ],
      [
        "expo-asset",
        {
          "assets": ["./assets/icons"]
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#eee",
          "image": "./assets/icon.png",
          "imageWidth": 200
        }
      ],
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "eebb6ee4-7931-4b76-a028-0dba660e4515"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/eebb6ee4-7931-4b76-a028-0dba660e4515"
    }
  }
}
