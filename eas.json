{
  "cli": {
    "version": ">= 5.9.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1",
        "EXPO_PUBLIC_BATTLE_SERVER_URL": "https://pootime-adventure-battle-server.onrender.com/"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_BATTLE_SERVER_URL": "https://pootime-adventure-battle-server.onrender.com/"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
